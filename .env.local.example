# Supabase (kopierade från din befintliga .env)
VITE_SUPABASE_URL=din-supabase-url
VITE_SUPABASE_ANON_KEY=din-supabase-anon-key

# OpenAI (kopierade från din befintliga .env)
VITE_OPENAI_API_KEY=din-openai-api-key

# Microsoft Graph Configuration
# Få dessa värden från Azure Portal -> App registrations
VITE_MS_CLIENT_ID=din-microsoft-client-id
VITE_MS_TENANT_ID=din-microsoft-tenant-id
VITE_MS_REDIRECT_URI=http://localhost:3001

# För production, uppdatera VITE_MS_REDIRECT_URI till din production URL
# VITE_MS_REDIRECT_URI=https://din-app-url.com
